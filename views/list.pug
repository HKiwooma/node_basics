extends layouts/main-layout.pug 
block content
    table.table
        thead.thead-dark
            tr
                th Firstname
                th Lastname
                th EmailAddress
                th Gender
                th Country
                th city
        tbody
            each user in users
                tr
                    td #{user.firstName}
                    td #{user.lastName}
                    td #{user.emailAddress}
                    td #{user.gender}
                    td #{user.country}
                    td #{user.city}
